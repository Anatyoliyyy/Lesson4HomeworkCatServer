<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns="http://java.sun.com/xml/ns/javaee"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GetMessage</servlet-name>
		<servlet-class>ua.kiev.prog.GetMessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetMessage</servlet-name>
		<url-pattern>/get</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddMessage</servlet-name>
		<servlet-class>ua.kiev.prog.AddMessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddMessage</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetRooms</servlet-name>
		<servlet-class>ua.kiev.prog.GetRoomsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetRooms</servlet-name>
		<url-pattern>/rooms</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>ua.kiev.prog.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
